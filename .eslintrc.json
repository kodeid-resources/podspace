{
  "parser": "babel-eslint",
  "extends": [ "airbnb", "plugin:react/recommended" ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/extensions": "always",
    "import/no-named-as-default-member": 0,
    "react/destructuring-assignment": [false]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
